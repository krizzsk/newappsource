<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:tools="http://schemas.android.com/tools&gt;" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:paddingLeft="24dp" android:paddingTop="40dp" android:paddingRight="24dp" android:paddingBottom="18dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <ImageView android:layout_gravity="center" android:id="@+id/skip_image" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="24dp" app:srcCompat="@drawable/img_empty_warning"/>
    <TextView android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="24dp" android:text="@string/payment_registration_doc_popup_title" style="?attr/dialogTitleStyle"/>
    <TextView android:id="@+id/message" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="18dp" android:text="@string/payment_registration_doc_popup_subtitle" style="?attr/dialogMessageStyle"/>
    <Button android:id="@+id/got_it_button" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/payment_registration_doc_popup_button" style="?attr/dialogPositiveButtonStyle"/>
</LinearLayout>
