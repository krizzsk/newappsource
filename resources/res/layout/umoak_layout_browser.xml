<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools&gt;" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@color/colorPrimary" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <com.cubic.umo.p045ad.playback.p046ui.views.AKWebView android:id="@+id/ak_web_view" android:background="@color/colorPrimary" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/layout_web_view_actions"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/layout_web_view_actions" android:background="@color/colorAccent" android:layout_width="match_parent" android:layout_height="50dp" android:weightSum="4" android:layout_alignParentBottom="true">
