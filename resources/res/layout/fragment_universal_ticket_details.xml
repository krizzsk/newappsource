<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.masabi.justride.sdk.p415ui.features.universalticket.components.FrostedScrollView android:id="@+id/frostedScrollView" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1"/>
    <ImageView android:id="@+id/dottedLine" android:layout_width="match_parent" android:layout_height="wrap_content" android:src="@drawable/com_masabi_justride_sdk_universal_ticket_dotted_line" android:layerType="software"/>
    <RelativeLayout android:id="@+id/navigationLayout" android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="60dp" android:paddingStart="@dimen/com_masabi_justride_sdk_universal_ticket_default_padding" android:paddingEnd="@dimen/com_masabi_justride_sdk_universal_ticket_default_padding">
        <Button android:id="@+id/backButton" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/com_masabi_justride_sdk_back" android:layout_centerVertical="true" android:layout_alignParentStart="true" style="@style/JustRideSDKUniversalNavigationButton"/>
        <Button android:id="@+id/termsButton" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/com_masabi_justride_sdk_terms" android:layout_centerVertical="true" android:layout_alignParentEnd="true" style="@style/JustRideSDKUniversalNavigationButton"/>
    </RelativeLayout>
</LinearLayout>
