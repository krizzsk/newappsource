<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="?attr/colorPrimary"/>
            <corners android:radius="500dp"/>
        </shape>
    </item>
    <item android:gravity="top|right|center_vertical|center_horizontal|center|end" android:top="-5dp" android:right="-4dp">
        <bitmap android:gravity="top|right|center_vertical|center_horizontal|center|end" android:src="@drawable/ic_check_mark_circ_18_blue"/>
    </item>
</layer-list>
