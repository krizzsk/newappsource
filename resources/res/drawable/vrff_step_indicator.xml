<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:maxLevel="0">
        <shape android:shape="oval">
            <solid android:color="@color/vrffPrimary02"/>
        </shape>
    </item>
    <item android:maxLevel="1">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="2dp" android:color="@color/vrffPrimary02"/>
        </shape>
    </item>
    <item android:maxLevel="2">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="2dp" android:color="@color/vrffDocument_selection_sepparator"/>
        </shape>
    </item>
</level-list>
