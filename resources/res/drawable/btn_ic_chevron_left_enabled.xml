<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <solid android:color="@android:color/transparent"/>
            <size android:height="36dp" android:width="2dp"/>
        </shape>
    </item>
    <item android:top="8dp" android:bottom="16dp">
        <rotate android:fromDegrees="45" android:toDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#9b9b9b"/>
                <corners android:radius="1dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp"/>
            </shape>
        </rotate>
    </item>
    <item android:top="16dp" android:bottom="8dp">
        <rotate android:fromDegrees="-45" android:toDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#9b9b9b"/>
                <corners android:radius="1dp" android:topLeftRadius="0dp" android:topRightRadius="0dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>
