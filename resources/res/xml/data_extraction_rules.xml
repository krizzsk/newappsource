    <cloud-backup>
        <exclude domain="root"/>
        <exclude domain="file"/>
        <exclude domain="database"/>
        <exclude domain="sharedpref"/>
        <exclude domain="external"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="root"/>
        <exclude domain="file"/>
        <exclude domain="database"/>
        <exclude domain="sharedpref"/>
        <exclude domain="external"/>
    </device-transfer>
</data-extraction-rules>
