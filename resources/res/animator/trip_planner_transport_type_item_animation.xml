<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_selected="true">
        <objectAnimator android:duration="0" android:valueTo="4dp" android:valueType="floatType" android:propertyName="elevation"/>
    </item>
    <item>
        <objectAnimator android:duration="0" android:valueTo="0dp" android:valueType="floatType" android:propertyName="elevation"/>
    </item>
</selector>
